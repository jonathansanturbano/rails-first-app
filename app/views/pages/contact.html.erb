<h1>Reach out to our fabulous batch</h1>

<form action="/contact" method="get">
  <input
  type="text"
  name="member"
  placeholder="Who are you looking for?"
  value="<%= params[:member] %>">
  <input type="submit">
</form>

<ul>
  <% @members.each do |member| %>
    <li>
      <%= member %>
    </li>
  <% end %>
</ul>
